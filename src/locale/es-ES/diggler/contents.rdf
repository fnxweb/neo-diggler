<?xml version="1.0"?>
<RDF:RDF xmlns:RDF="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:chrome="http://www.mozilla.org/rdf/chrome#">

  <!-- list all the packages being supplied by this jar -->
  <RDF:Seq about="urn:mozilla:locale:root">
    <RDF:li resource="urn:mozilla:locale:en-US"/>
	 <RDF:li resource="urn:mozilla:locale:es-ES"/>
  </RDF:Seq>

  <!-- locale information -->
  <RDF:Description about="urn:mozilla:locale:en-US"
        chrome:displayName="English(US)"
        chrome:author="app_author"
        chrome:name="en-US"
        chrome:previewURL="http://www.mozilla.org/locales/en-US.gif">
	<RDF:Description about="urn:mozilla:locale:es-ES"
		  chrome:displayName="Castellano"
		  chrome:author="Jorge Salamero Sanz"
		  chrome:name="es-ES"
		  chrome:previewURL="http://www.mozilla.org/locales/es-ES.gif">
    <chrome:packages>
      <RDF:Seq about="urn:mozilla:locale:en-US:packages">
        <RDF:li resource="urn:mozilla:locale:en-US:diggler"/>
      </RDF:Seq>
		<RDF:Seq about="urn:mozilla:locale:es-ES:packages">
		  <RDF:li resource="urn:mozilla:locale:es-ES:diggler"/>
		</RDF:Seq>
    </chrome:packages>
  </RDF:Description>
</RDF:RDF>

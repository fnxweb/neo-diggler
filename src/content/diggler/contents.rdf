<?xml version="1.0"?>

<RDF:RDF xmlns:RDF="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:chrome="http://www.mozilla.org/rdf/chrome#">

  <!-- list all the packages being supplied by this jar -->
  <RDF:Seq about="urn:mozilla:package:root">
    <RDF:li resource="urn:mozilla:package:diggler"/>
  </RDF:Seq>

  <!-- package information -->
  <RDF:Description about="urn:mozilla:package:diggler"
        chrome:displayName="Diggler"
        chrome:author="Adam Lock"
        chrome:authorURL="http://diggler.mozdev.org/"
        chrome:name="diggler"
        chrome:extension="true"
        chrome:settingsURL="chrome://diggler/content/diggler-fb-settings.xul">
  </RDF:Description>

  <!-- overlay information -->
  <RDF:Seq about="urn:mozilla:overlays">
    <!-- Firebird -->
    <RDF:li resource="chrome://browser/content/browser.xul"/>
    <!-- Mozilla -->
    <RDF:li resource="chrome://navigator/content/navigator.xul"/>
    <RDF:li resource="chrome://communicator/content/pref/pref-navigator.xul"/>
    <RDF:li resource="chrome://communicator/content/pref/preftree.xul"/>
  </RDF:Seq>

  <!-- Firefox -->
  <RDF:Seq about="chrome://browser/content/browser.xul">
    <RDF:li>chrome://diggler/content/digglerOverlay.xul</RDF:li>
  </RDF:Seq>

  <!-- Mozilla -->
  <RDF:Seq about="chrome://navigator/content/navigator.xul">
    <RDF:li>chrome://diggler/content/digglerOverlay.xul</RDF:li>
  </RDF:Seq>

  <RDF:Seq about="chrome://communicator/content/pref/pref-navigator.xul">
    <RDF:li>chrome://diggler/content/digglerPrefOverlay.xul</RDF:li>
  </RDF:Seq>

  <RDF:Seq about="chrome://communicator/content/pref/preftree.xul">
    <RDF:li>chrome://diggler/content/digglerPrefOverlay.xul</RDF:li>
  </RDF:Seq>

</RDF:RDF>

